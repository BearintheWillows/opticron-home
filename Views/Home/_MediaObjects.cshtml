@model List<MediaObjects>

<section class="media-objects-container grid-x grid-margin-x small-up-1 medium-up-2 large-up-4">
@foreach(var item in Model)
{
    <div class="cell mediaObject">
        <div class="expanded">
            <h4 style="color: #a2a2a4;">@item.Title</h4>
        </div>
        <div class="expanded">
            <img src="@item.ImagePath" alt="@item.ImageAlt">
        </div>
        <div class="expanded">
            @item.Text
        </div>
        <div class="expanded">
            <button class="button">@item.ButtonText</button>
        </div>
    </div>
}
</section>